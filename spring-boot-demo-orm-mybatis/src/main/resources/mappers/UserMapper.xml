<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ycb.demo.mapper.UserMapper">
    <insert id="saveUser">
        insert into user values
        (#{user.id},
        #{user.name},
        #{user.password},
        #{user.salt},
        #{user.email},
        #{user.phoneNumber},
        #{user.status},
        #{user.createTime},
        #{user.lastLoginTime},
        #{user.lastUpdateTime})
    </insert>

    <delete id="deleteById">
        DELETE
        FROM `user`
        WHERE `id` = #{id}
    </delete>
</mapper>